{
  "name": "szepeviktor/persistent-admin-notices",
  "type": "library",
  "description": "Persistent admin notices for WordPress.",
  "license": "MIT",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "szepeviktor/phpstan-wordpress": "^0.2.1",
    "phpstan/phpstan-shim": "^0.11.8",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "WordPress\\Admin\\": "src/"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyze",
    "phpcs": "phpcs -p --standard=PSR12 src/"
  }
}
